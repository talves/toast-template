{
  "name": "toast-template",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "private": true,
  "author": "Tony Alves (@talves)",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./.tmp && rm -rf ./public",
    "start": "NODE_ENV=production yarn gulp",
    "postinstall": "node ./postinstall.js",
    "build": "yarn postinstall && yarn build:css && yarn build:site",
    "build:css": "postcss --config legacy-commonjs src/index.css -o public/styles.css --env production",
    "build:site": "toast incremental ."
  },
  "dependencies": {
    "@mdx-js/preact": "^1.6.22",
    "@toastdotdev/mdx": "^0.2.4",
    "autoprefixer": "10.3.1",
    "console.table": "^0.10.0",
    "esinstall": "^1.1.7",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "preact": "^10.5.14",
    "pretty-bytes": "^5.6.0",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^2.0.2",
    "toast": "^0.3.47"
  },
  "devDependencies": {
    "browser-sync": "^2.26.13",
    "execa": "^4.1.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0"
  },
  "resolutions": {
    "@mdx-js/mdx": "2.0.0-next.8",
    "remark-mdx": "2.0.0-next.8"
  }
}