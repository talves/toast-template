{
  "name": "toast-template",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "private": true,
  "author": "Tony Alves (@talves)",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./.tmp && rm -rf ./public",
    "start": "NODE_ENV=production yarn gulp",
    "postinstall": "node ./postinstall.js",
    "build": "yarn postinstall && yarn build:css && yarn build:site",
    "build:css": "postcss --config legacy-commonjs src/index.css -o public/styles.css --env production",
    "build:site": "toast incremental ."
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.1.3",
    "@mdx-js/preact": "^2.1.3",
    "@toastdotdev/mdx": "^0.3.2",
    "console.table": "0.10.0",
    "esinstall": "1.1.7",
    "postcss": "8.4.16",
    "postcss-cli": "8.3.1",
    "preact": "10.10.6",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^3.1.8",
    "toast": "^0.5.4"
  },
  "devDependencies": {
    "autoprefixer": "10.4.8",
    "browser-sync": "^2.26.13",
    "execa": "^4.1.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "pretty-bytes": "^5.5.0"
  }
}